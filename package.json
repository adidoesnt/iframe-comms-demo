{
  "name": "iframe-experiments",
  "version": "1.0.0",
  "description": "practice repository for iframe and parent site communication",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "npm run build-dev && npm run open",
    "open": "concurrently \"http-server -a localhost -p 8080\" \"open http://localhost:8080/dist\""
  },
  "author": "Aditya Banerjee",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "http-server": "^14.1.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
